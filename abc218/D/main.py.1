tokens = (i for line in iter(input, "") for i in line.split())


def solve(N: int, x_list: "List[int]", y_list: "List[int]"):
    xy_set = {(x,y) for x,y in zip(x_list,y_list)}
    ans = 0
    for i in range(N-1):
        for j in range(i+1,N):
            x1,y1 = x_list[i],y_list[i]
            x2,y2 = x_list[j],y_list[j]
            if x1 == x2 or  y1 == y2:
                continue
            if (x1,y2) in xy_set and (x2,y1) in xy_set:
                ans += 1
    print(ans//2)

def main():
    N = int(next(tokens))  # type: int
    x = [int()] * (N)  # type: "List[int]"
    y = [int()] * (N)  # type: "List[int]"
    for i in range(N):
        x[i] = int(next(tokens))
        y[i] = int(next(tokens))
    solve(N, x, y)
    return

main()
