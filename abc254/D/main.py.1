def solve(N: int):

    t = list(range(1,N+1))

    for i in range(N):
        j = 2
        while t[i] >= j*j:
            if t[i] % (j*j)==0:
                while t[i] % (j*j)==0:
                    t[i] //= j*j
            j+= 1
    
    count_t = Counter(t)
    ans = 0
    for v in count_t.values():
        ans += v*v
    
    print(ans)


def main():
    N = int(next(tokens))  # type: int
    solve(N)
    return

main()
